{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "protect-resource",
  "description": "Middleware that deals with protection of resources. Supports roles and works transparently with all login vectors.",
  "version": "0.2.1",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/codedoctor/node-protect-resource.git"
  },
  "bin": {},
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "keywords": [
    "api",
    "rest"
  ],
  "scripts": {
    "prepublish": "node_modules/.bin/grunt coffee",
    "test": "node_modules/.bin/grunt coffee;NODE_ENV=test node_modules/.bin/mocha --timeout 10000 -R spec test/*.coffee",
    "docs": "node_modules/.bin/codo -i ./src -o ./docs"
  },
  "engines": {},
  "dependencies": {
    "underscore": ">=1.5.2"
  },
  "devDependencies": {
    "coffee-script": "1.6.3",
    "should": "2.0.2",
    "mocha": "1.13.0",
    "codo": "1.9.0",
    "grunt": "0.4.1",
    "grunt-cli": "0.1.9",
    "grunt-contrib-clean": "0.5.0",
    "grunt-contrib-copy": "0.4.1",
    "grunt-contrib-watch": "0.5.3",
    "grunt-release": "0.6.0",
    "grunt-contrib-coffee": "0.7.0"
  },
  "bugs": {
    "url": "http://github.com/codedoctor/node-protect-resource/issues"
  },
  "licenses": [
    {
      "type": "MIT",
